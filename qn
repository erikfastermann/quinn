a :: [[[x int] [y int]] string] {
	tmp = strconv_atoi y
	n = if (a < b) { 1 } else { 2 }
	[(x + tmp) (n + tmp)]
	for i in [a b c] {
	}
	b = {
		x
		y
	}
	(a b | c ()) | d e
	a ((b c | d) | e f) g

	a
	(()-b)
	y = match x [
		[Some x] {}
		[None] {}
	]
}

a = b
(a b) = c


